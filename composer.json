{
    "name": "cjmellor/boost-ide-bundle",
    "description": "Extended IDE support bundle for Laravel Boost. Multiple development environments, one package.",
    "keywords": [
        "laravel",
        "boost-ide-bundle"
    ],
    "homepage": "https://github.com/cjmellor/boost-ide-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Mellor",
            "email": "cmellor@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.49.0|^11.45.3|^12.28.1"
    },
    "require-dev": {
        "laravel/boost": "^1.4",
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^8.36.0|^9.15.0|^10.6",
        "pestphp/pest": "^2.36.0|^3.8.4|^v4.1.2",
        "pestphp/pest-plugin-arch": "^v2.7.0|^3.0|^4.0",
        "pestphp/pest-plugin-laravel": "^v2.4.0|^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Cjmellor\\BoostIdeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cjmellor\\BoostIdeBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ],
        "post-update-cmd": [],
        "test": "vendor/bin/pest",
        "lint": "./vendor/bin/pint",
        "lint:test": "./vendor/bin/pint --test"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cjmellor\\BoostIdeBundle\\BoostIdeBundleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
